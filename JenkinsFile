pipeline {
    agent any

    stages {
        stage('Building'){
        	steps{
        		sh 'pwd'
        		sh 'chmod 777 build.sh'
        		sh 'ls -alrt'
        		sh './build.sh'
        	}       	
        }
        stage('Artifacts '){
        	steps{ 
        		archiveArtifacts artifacts: '*serialize'
        	}       	
        }
    }
}
