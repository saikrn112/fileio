pipeline {
    agent any

    stages {
        stage('Building'){
        	steps{
        		sh 'pwd'
        		sh 'chmod 777 build.sh'
        		sh 'ls -alrt'
        		sh './build.sh'
        	}       	
        }
        stage('Ansible Playbook Lauch'){
            steps{
                sh 'echo $WORKSPACE'
            }
        }
        stage('Artifacts '){
        	steps{ 
        		archiveArtifacts artifacts: 'build/src/serialize'
        	}       	
        }
    }
}
