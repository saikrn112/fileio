pipeline {
    agent any

    stages {

        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
        stage('Actually building'){
        	steps{
        		sh 'pwd'
        		sh 'ls -alrt'
        		sh './build.sh'
        	}
        	
        }

    }
}
